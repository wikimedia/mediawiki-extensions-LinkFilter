{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery"
	],
	"globals": {
		"mediaWiki": false,
		"mw": false,
		"jQuery": false
	},
	"rules": {
		"no-implicit-globals": "warn",
		"camelcase": "warn",
		"eqeqeq": "warn",
		"no-alert": "warn",
		"no-unused-vars": "warn",
		"vars-on-top": "warn",
		"no-jquery/no-global-selector": "warn",
		"no-jquery/no-animate-toggle": "warn",
		"no-jquery/variable-pattern": "warn"
	}
}
